{
  "expo": {
    "name": "xianxu",
    "slug": "native",
    "version": "1.0.0",
    "scheme": "xianxu",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "app.xianxu"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
        "experiments": {
      "typedRoutes": true
    },
    "plugins": ["expo-router"]
  }
}
